<metal:main metal:use-macro="load: master.pt">
    <metal:slot metal:fill-slot="content">
        <h2>Requests from shopify:</h2>
        <br><br>
        <table>
            <tr tal:repeat="request requests">
                <td>${request.uuid}&nbsp;&nbsp;&nbsp;<br></td>
                <td>${request.date}&nbsp;&nbsp;&nbsp;<br></td>
                <td>${request.json}&nbsp;&nbsp;&nbsp;<br></td>
            </tr>
        </table>
    </metal:slot>
</metal:main>